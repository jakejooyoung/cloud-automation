{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": [
                "arn:aws:s3:::npgains.letsencrypt/*",
                "arn:aws:s3:::npgains.nginxconfig/*",
                "arn:aws:s3:::npgains.views/*"
            ]
        }
    ]
}